FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/DJI-SDK-iOS/iOS_Mobile_SDK" "${PODS_ROOT}/DJIWidget/FFmpeg"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "$(PODS_ROOT)/DJIWidget/FFmpeg/FFmpeg.framework/Headers" "$(PODS_ROOT)/Headers/Public/DJIWidget/FFmpeg/.."/** "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/DJI-SDK-iOS" "${PODS_ROOT}/Headers/Public/DJIWidget"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/DJIWidget"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/DJI-SDK-iOS" -isystem "${PODS_ROOT}/Headers/Public/DJIWidget"
OTHER_LDFLAGS = $(inherited) -ObjC -l"DJIWidget" -framework "DJISDK" -framework "FFmpeg"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
